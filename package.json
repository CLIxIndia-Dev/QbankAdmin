{
  "name": "QBankAdmin",
  "version": "0.0.1",
  "description": "Admin tool for MIT's QBank",
  "engines": {
    "node": "5.10.1"
  },
  "scripts": {
    "postinstall": "cd ./client && npm install",
    "build": "cd ./client && npm run build",
    "build_dev": "cd ./client && npm run build_dev",
    "release": "cd ./client && npm run release",
    "hot": "cd ./client && npm run hot",
    "live": "cd ./client && node server.js",
    "test": "cd ./client && npm run test",
    "coveralls": "cd ./client && npm run coveralls",
    "install": "cd authentication && npm install && cd ../authorization && npm install && cd ../lambda/offer && npm install && cd ../proxy && npm install && cd ../items && npm install",
    "lint": "eslint . || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atomicjolt/QBankAdmin"
  },
  "keywords": [
    "QBank Admin"
  ],
  "author": "atomicjolt",
  "license": "MIT",
  "homepage": "https://github.com/atomicjolt/react_client_starter_app",
  "dependencies": {
    "del": "^2.2.0"
  },
  "devDependencies": {
    "async": "^2.0.0-rc.3",
    "aws-sdk": "^2.2.48",
    "chai": "^3.5.0",
    "dirty-chai": "^1.2.2",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "mocha": "^2.4.5",
    "nock": "^7.7.2",
    "serverless-authentication": "^0.4.4"
  },
  "private": false
}
